{
  "name": "Jump2",
  "version": "1.1",
  "description": "Jump to a header",
  "manifest_version": 3,
  "background": {
    "service_worker": "service-worker.js"
  },
  "commands": {
    "openUI": {
      "description": "Open UI",
      "suggested_key": {
        "default": "Ctrl+Shift+H",
        "mac": "MacCtrl+Shift+H"
      }
    },
    "_execute_action": {
      "suggested_key": {
        "default": "Ctrl+Shift+H",
        "mac": "MacCtrl+Shift+H"
      },
      "description": "Start the extension"
    }
  },
  "action": {
    "default_popup": "index.html"
  },
  "permissions": ["scripting","tabs","storage"],
  "host_permissions": ["http://*/*", "https://*/*"]
}
